<h1>Sign up</h1>

<%= form_with(model: @user, url: users_path, method: "post", local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.email_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <br>
  <%= f.submit "新規登録" %>
<% end %>
